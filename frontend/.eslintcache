ed: !prevState.isExpanded }));\r\n  };\r\n\r\n  onToggleAccordion = (item) => {\r\n    if (this.state.isItemOpen === item) {\r\n      this.setState({ isItemOpen: null });\r\n    } else {\r\n      this.setState({ isItemOpen: item });\r\n    }\r\n  };\r\n\r\n  getAccordion = (item, content) => {\r\n    let title = item[0].toUpperCase() + item.substring(1);\r\n    return (\r\n      <div className=\"accordion-container\">\r\n        <div\r\n          className=\"accordion-toggle\"\r\n          onClick={() => this.onToggleAccordion(item)}\r\n        >\r\n          {title}\r\n          {this.state.isItemOpen === item ? <ChevronUp /> : <ChevronDown />}\r\n        </div>\r\n        <div\r\n          className={`accordion-menu ${\r\n            this.state.isItemOpen === item ? \"expanded\" : \"\"\r\n          }`}\r\n        >\r\n          {content.map((c, index) => (\r\n            <a\r\n              key={index}\r\n              href={c.to}\r\n              onClick={() => {\r\n                this.setState({ isExpanded: null, isItemOpen: null });\r\n                if (c.title === \"Litepaper\") {\r\n                  window.open(\r\n                    \"https://metafactory.ai\",\r\n                    \"_blank\"\r\n                  );\r\n                }\r\n              }}\r\n            >\r\n              {c.title}\r\n            </a>\r\n          ))}\r\n        </div>\r\n      </div>\r\n    );\r\n  };\r\n\r\n  scrollTo = (item) => {\r\n    // Main app\r\n    return (\r\n      <div\r\n        className=\"menu-item\"\r\n        onClick={() => {\r\n          this.setState({ isExpanded: null, isItemOpen: null });\r\n          document\r\n            .getElementById(item.to)\r\n            ?.scrollIntoView({ behavior: \"smooth\" });\r\n        }}\r\n      >\r\n        {item.title}\r\n      </div>\r\n    );\r\n  };\r\n\r\n  getLink = (item) => {\r\n    // Other apps\r\n    return (\r\n      <a\r\n        href={item.to}\r\n        className=\"menu-item\"\r\n        onClick={() => {\r\n          this.setState({ isExpanded: null, isItemOpen: null });\r\n        }}\r\n      >\r\n        {item.title}\r\n      </a>\r\n    );\r\n  };\r\n\r\n  render() {\r\n    // Small to medium navigation\r\n    const XSNav = () => {\r\n      return (\r\n        <>\r\n          <div className=\"xs-nav-toggle\" onClick={this.onToggleDrawer}>\r\n            {!this.state.isExpanded ? <Menu /> : <X />}\r\n          </div>\r\n          <div\r\n            className={`xs-nav-menu ${this.state.isExpanded ? \"expanded\" : \"\"}`}\r\n          >\r\n            { this.getLink(buy) }\r\n            { this.getAccordion(\"resources\", resources) }\r\n            { this.getAccordion(\"social\", social) }\r\n          </div>\r\n        </>\r\n      );\r\n    };\r\n    // Large navigation\r\n    const LGNav = () => {\r\n      return (\r\n        <>\r\n          <div className=\"lg-nav-menu\">\r\n            { this.getLink(buy) }\r\n            { this.getAccordion(\"resources\", resources) }\r\n            { this.getAccordion(\"social\", social) }\r\n          </div>\r\n        </>\r\n      );\r\n    };\r\n\r\n    return (\r\n      <div className=\"header-container\">\r\n        <div className=\"header-content\">\r\n          <a href=\"https://metafactory.ai\" className=\"logo-container\">\r\n            <div className=\"logo-img\">\r\n              <img src={Logo} alt=\"logo\" />\r\n            </div>\r\n            <div className=\"logo-title\">MetaFactory</div>\r\n          </a>\r\n          {this.state.isSmall || this.state.isMedium ? <XSNav /> : <LGNav />}\r\n        </div>\r\n      </div>\r\n    );\r\n  }\r\n}\r\n","/Users/unico/Documents/GitHub/robot-distributions/frontend/src/routes/loader/index.js",[],"/Users/unico/Documents/GitHub/robot-distributions/frontend/src/components/footer/medias.js",[],"/Users/unico/Documents/GitHub/robot-distributions/frontend/src/components/airdrop/elements.js",[],"/Users/unico/Documents/GitHub/robot-distributions/frontend/src/componen